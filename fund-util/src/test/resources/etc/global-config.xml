<?xml version="1.0" encoding="UTF-8"?>
<!--此文件放置目录: windows(D:\etc\global.conf), mac(/etc/mtime/global.conf), linux("/home/mtime/etc/global.conf")-->
<global>
    <!-- etcd地址(new) -->
    <option key="etcd.address" value="http://192.168.54.26:2379,http://192.168.54.27:2379,http://192.168.54.29:2379" />
    <!-- 日志存储根目录 -->
    <option key="log.path" value="/home/piaoshen/log" />
    <!-- 服务注册类型 -->
    <option key="rpc.register.type" value="etcd" />
    <!-- 是否启用服务注册 -->
    <option key="rpc.register.enabled" value="false" />
    <!-- 服务注册 IP -->
    <option key="rpc.register.ip" value="192.168.89.222" />
    <!-- 是否启用服务发现 -->
    <option key="rpc.discovery.enabled" value="true" />
</global>